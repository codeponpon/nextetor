{
  "name": "nextetor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "-------------------------------- CLEAR ----": "----",
    "clean": "rimraf .next/ out/",
    "-------------------------------- LINT ----": "----",
    "tsc-check": "tsc -v && tsc",
    "format": "prettier --write src/**/*.ts{,x}",
    "-------------------------------- DEV ----": "----",
    "dev": "MODE=development node bin/dev",
    "-------------------------------- PROD ----": "----",
    "build": "MODE=production node bin/build",
    "start": "MODE=production node bin/start",
    "-------------------------------- STAGING ----": "----",
    "build:staging": "MODE=staging node bin/build",
    "start:staging": "MODE=staging node bin/start",
    "-------------------------------- CODEGEN ----": "----",
    "codegen": "graphql-codegen --config codegen.yml",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@apollo/client": "^3.3.20",
    "@babel/runtime": "^7.14.6",
    "@prisma/client": "2.26.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.6",
    "@types/lodash": "^4.14.170",
    "@types/nprogress": "^0.2.0",
    "@types/react-redux": "^7.1.16",
    "@types/redux-logger": "^3.0.8",
    "antd": "^4.16.5",
    "apollo-server-micro": "^2.25.2",
    "bcrypt": "^5.0.1",
    "dayjs": "^1.10.5",
    "faker": "^5.5.3",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "next": "^11.0.1",
    "next-redux-wrapper": "^7.0.2",
    "nprogress": "^0.2.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-use": "^17.2.4",
    "redux": "^4.1.0",
    "serverless-mysql": "^1.5.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-react-apollo": "^2.2.8",
    "@graphql-codegen/typescript-resolvers": "1.19.5",
    "@next/bundle-analyzer": "^11.0.1",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.13",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less-vars-to-js": "^1.3.0",
    "next-plugin-antd-less": "^1.3.0",
    "prettier": "^2.3.2",
    "prisma": "2.26.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
